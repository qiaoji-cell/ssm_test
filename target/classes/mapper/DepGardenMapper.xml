<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.axjy.mapper.sys.DepGardenMapper">
  <delete id="delete">
    delete from kms_dep_garden where garden_id = #{record.gardenId} and dep_id = #{record.depId}
  </delete>


  <resultMap id="BaseResultMap" type="com.axjy.pojo.sys.DepGarden">
    <result column="dep_id" jdbcType="BIGINT" property="depId" />
    <result column="garden_id" jdbcType="BIGINT" property="gardenId" />
  </resultMap>
  <insert id="insert" parameterType="com.axjy.pojo.sys.DepGarden">
    insert into kms_dep_garden (dep_id, garden_id)
    values (#{depId,jdbcType=BIGINT}, #{gardenId,jdbcType=BIGINT})
  </insert>
  <insert id="insertSelective" parameterType="com.axjy.pojo.sys.DepGarden">
    insert into kms_dep_garden
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="depId != null">
        dep_id,
      </if>
      <if test="gardenId != null">
        garden_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="depId != null">
        #{depId,jdbcType=BIGINT},
      </if>
      <if test="gardenId != null">
        #{gardenId,jdbcType=BIGINT},
      </if>
    </trim>
  </insert>
</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.axjy.mapper.sys.RoleGardenMapper">

  <delete id="delete">
    delete from kms_role_garden where garden_id = #{gardenId} and role_id in
    <foreach collection="roleIds" item="roleId" open="(" separator="," close=")">
        #{roleId}
    </foreach>
  </delete>

  <resultMap id="BaseResultMap" type="com.axjy.pojo.sys.RoleGarden">
    <result column="role_Id" jdbcType="BIGINT" property="roleId" />
    <result column="garden_id" jdbcType="BIGINT" property="gardenId" />
  </resultMap>
  <insert id="insert" parameterType="com.axjy.pojo.sys.RoleGarden">
    insert into kms_role_garden (role_Id, garden_id)
    values (#{roleId,jdbcType=BIGINT}, #{gardenId,jdbcType=BIGINT})
  </insert>
  <insert id="insertSelective" parameterType="com.axjy.pojo.sys.RoleGarden">
    insert into kms_role_garden
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="roleId != null">
        role_Id,
      </if>
      <if test="gardenId != null">
        garden_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="roleId != null">
        #{roleId,jdbcType=BIGINT},
      </if>
      <if test="gardenId != null">
        #{gardenId,jdbcType=BIGINT},
      </if>
    </trim>
  </insert>
</mapper>